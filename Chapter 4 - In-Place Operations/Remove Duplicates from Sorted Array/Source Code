/*        LANGUAGE C        */

int removeDuplicates(int* nums, int numsSize){
    
    if(numsSize < 2) {
        return numsSize;    
    }
    
    int pointerOne = 0, pointerTwo = 0;
    for(int i=0 ; i<(numsSize - 1) ; i++) {
        
        if(nums[i] == nums[i + 1]) {
            pointerTwo++;
        }


        else {
            nums[++pointerOne] = nums[++pointerTwo];
        }
    }
    return (++pointerOne);
}
